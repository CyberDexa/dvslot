# DVSlot Production Environment Variables for Vercel
# Copy these to your Vercel project environment variables

# Supabase Configuration (REQUIRED)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# App Configuration
EXPO_PUBLIC_API_BASE_URL=https://your-domain.vercel.app
EXPO_PUBLIC_WEB_URL=https://your-domain.vercel.app
EXPO_PUBLIC_APP_NAME=DVSlot
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_ENVIRONMENT=production

# Optional Features (can be left as defaults)
EXPO_PUBLIC_ENABLE_LOCATION_TRACKING=true
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false
EXPO_PUBLIC_ENABLE_DEBUG_LOGS=false

# Support Information
EXPO_PUBLIC_SUPPORT_EMAIL=support@dvslot.com
EXPO_PUBLIC_PRIVACY_POLICY_URL=https://your-domain.vercel.app/privacy
EXPO_PUBLIC_TERMS_OF_SERVICE_URL=https://your-domain.vercel.app/terms
EXPO_PUBLIC_CONTACT_URL=https://your-domain.vercel.app/contact
